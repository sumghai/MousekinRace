<?xml version="1.0" encoding="utf-8"?>
<Defs>

	<!-- Note on initialWillRange and initialResistanceRange:
		- Grimalkin are loyal to the Grimalkin Dominion, and are thus very difficult or nearly impossible to recruit or enslave
        - The Anomaly DLC does come with a Brainwipe ritual, but that is an Outside Context Problem for Grimalkins anyway 		
	-->
	
	<PawnKindDef Name="Grimalkin_BasePawnKind" Abstract="True">
		<race>Grimalkin</race>
		<combatPower>50</combatPower>
		<nameMaker>NamerPerson_Grimalkin</nameMaker>
		<defaultFactionType>Grimalkin_Faction</defaultFactionType>
		<invFoodDef>Grimalkin_MealRations</invFoodDef>
		<invNutrition>3</invNutrition>
		<backstoryCryptosleepCommonality>0</backstoryCryptosleepCommonality>
		<forceNormalGearQuality>true</forceNormalGearQuality>
		<forceWeaponQuality>Normal</forceWeaponQuality>
		<apparelMoney>1000~1500</apparelMoney>
		<weaponMoney>1000~1500</weaponMoney>
		<apparelIgnoreSeasons>true</apparelIgnoreSeasons>
		<initialWillRange>99999~99999</initialWillRange>
		<initialResistanceRange>99999~99999</initialResistanceRange>
		<fixedGender>Male</fixedGender> <!-- Usually male unless explicitly overridden -->
		<xenotypeSet>
			<xenotypeChances>
				<Grimalkin_XenotypeGrimalkin MayRequire="Ludeon.Rimworld.Biotech">1</Grimalkin_XenotypeGrimalkin>
			</xenotypeChances>
		</xenotypeSet>
	</PawnKindDef>
	
	<PawnKindDef Name="Grimalkin_InfantryBasicPawnKind" ParentName="Grimalkin_BasePawnKind" Abstract="True">
		<apparelTags>
			<li>GrimalkinInfantryBasic</li>
		</apparelTags>
		<apparelRequired>
			<li>Grimalkin_HelmetInfantry</li>
			<li>Grimalkin_UniformInfantry</li>
		</apparelRequired>
		<specificApparelRequirements>
			<li>
				<bodyPartGroup>UpperHead</bodyPartGroup>
				<apparelLayer>Overhead</apparelLayer>
				<apparelDef>Grimalkin_HelmetInfantry</apparelDef>
			</li>
			<li>
				<bodyPartGroup>Torso</bodyPartGroup>
				<apparelLayer>OnSkin</apparelLayer>
				<apparelDef>Grimalkin_UniformInfantry</apparelDef>
			</li>
		</specificApparelRequirements>
	</PawnKindDef>
	
