<?xml version="1.0" encoding="utf-8"?>
<Defs>
	
	<RulePackDef>
		<defName>NamerFaction_MousekinKingdom</defName>
		<rulePack>
			<rulesStrings>
				<li>r_name->The [KingdomKind] of [Herb]</li>
				<li>r_name->The [KingdomKind] of the [Flower]</li>
				<li>r_name->[KingdomKind] of [Herb]</li>
				<li>r_name->[KingdomKind] of the [Flower]</li>
				<li>r_name->The [Flower] [KingdomKind]</li>
				<li>KingdomKind(p=4)->Kingdom</li>
				<li>KingdomKind->Empire</li>
